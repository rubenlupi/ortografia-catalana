  "rules": [
    {
      "id": 1,
      "message": "La lletra b es manté davant de l i r, així com en mots com blanc, bleda, agradable, i altres similars.",
      "rule": "b -> b / _{l, r}",
      "ex": "blanc, bleda, agradable, fibló, moble"
    },
    {
      "id": 2,
      "message": "La lletra b es manté darrere de m en paraules com ambient, combat, comboi, i altres similars.",
      "rule": "b -> b / m_",
      "ex": "ambient, combat, comboi, també, embotit"
    },
    {
      "id": 3,
      "message": "La lletra b es manté davant de vocal quan alterna amb p entre paraules d'una mateixa família o formes d'un mateix paradigma.",
      "rule": "b -> b / _V [p-alternation]",
      "ex": "abella (cf. apicultura), acabar (cf. cap), saber (cf. sap), cubell (cf. cup)"
    },
    {
      "id": 4,
      "message": "La lletra b es manté a final de síl·laba en contextos com absolut, cabdell, cobdícia, i altres similars.",
      "rule": "b -> b / _C",
      "ex": "absolut, cabdell, cobdícia, dissabte, dubte"
    },
    {
      "id": 5,
      "message": "La lletra v es manté darrere de n en paraules com canvi, convèncer, enviar, i altres similars.",
      "rule": "v -> v / n_",
      "ex": "canvi, convèncer, enviar, envoltar, invent"
    },
    {
      "id": 6,
      "message": "La lletra v es manté darrere de d en paraules com advent, advers, advertir, i altres similars.",
      "rule": "v -> v / d_",
      "ex": "advent, advers, advertir, advocat, inadvertit"
    },
    {
      "id": 7,
      "message": "La lletra v alterna amb u davant de vocals en mots d'una mateixa família o formes d'un mateix paradigma.",
      "rule": "v -> v / _V [u-alternation]",
      "ex": "activa (cf. actiu), blava (cf. blau), nevar (cf. neu), nova (cf. nou)"
    },
    {
      "id": 8,
      "message": "La lletra v es manté en les terminacions de l'imperfet d'indicatiu de la primera conjugació.",
      "rule": "v -> v / _{imperfet}",
      "ex": "parlava, escoltaves, miràvem, tancaven"
    },
    {
      "id": 9,
      "message": "La lletra v es manté en les formes rizotòniques dels presents d'indicatiu, subjuntiu, i imperatiu del verb anar.",
      "rule": "v -> v / _{rizotònic}",
      "ex": "vaig, vas, va, van, vagi, vagis"
    },
    {
      "id": 10,
      "message": "La lletra v es manté en manlleus adaptats amb w en els mots originals.",
      "rule": "v -> v / {manlleu}",
      "ex": "grauvaca, sandvitx, valquíria, vàter, Valhalla"
    },
    {
      "id": 11,
      "message": "La lletra b es manté en paraules que comencen amb 'bi-' seguit de una consonant.",
      "rule": "b -> b / bi_C",
      "ex": "bicicleta, bicolor, bimensual"
    }
  ],
